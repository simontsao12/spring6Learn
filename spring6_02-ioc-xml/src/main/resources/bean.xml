<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <!--1 獲取bean User物件創建-->
    <bean id="user" class="com.atsimoncc.spring6.iocxml.User"/>
    <!--只根據類型獲取bean會出錯-->
    <bean id="user1" class="com.atsimoncc.spring6.iocxml.User"/>
    <!--2 一個介面實現類獲取過程-->
    <bean id="userDAOImpl" class="com.atsimoncc.spring6.iocxml.bean.UserDAOImpl"/>
    <bean id="personDAOImpl" class="com.atsimoncc.spring6.iocxml.bean.PersonDAOImpl"/>
</beans>